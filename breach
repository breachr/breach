  if any([pcl, pjl, ps]):
    with open("printer_support.txt", "w") as fil:
      fil.write(F"{target} {ps} {pjl} {pcl}")
  if not any([pcl, pjl, ps]):
    pass
    # TODO: read html title / printername from http_tits.txt
    # TODO: get printer name from snmp (./snmp/target)
