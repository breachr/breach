  run_dep_check('snmpbrute -h | grep -q Usage')
  run_dep_check('snmpwalk -h 2>&1| grep APPOPTS')  # sudo apt install snmp
  run_dep_check('snmpbulkwalk -h 2>&1| grep APPOPTS')
  
  print(Breach.cyan + "\n[>>>] printer" + Breach.endc)
