    # loot_snmp("sed 's/.*: //' | sed 's/.* = //' | sed 's/\"\"/-/' | sed 's/^\"//' | sed 's/\"$//'", mib.prntLangPretty, printer.prntLangPretty)
    # loot_snmp("sed 's/.*STRING: \"//' | sed 's/\"//'", mib.name, printer.name)
