  os.system("clear")
  print("""
                    .:odxxddl:,.
                  'lxOkol::;;,,,'.
               ,xXWKOkOKXXXX0xl;.
             .dNMMMMMWXOdooooddxdc;.
            ,OWMMWXkl;.           ..
           cXMWKd:.
